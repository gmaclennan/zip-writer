    "@vitest/browser": "^4.0.7",
    "@vitest/browser-playwright": "^4.0.7",
    "@zip.js/zip.js": "^2.8.8",
    "archiver": "^7.0.1",
    "eslint": "^9.39.1",
    "execa": "^9.6.0",
    "fflate": "^0.8.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
